{
  "name": "ui-kit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "typings": "./dist/index.d.ts",
  "main": "./dist/ui-kit.cjs.js",
  "module": "./dist/ui-kit.es.js",
  "exports": {
    "./forms": {
      "types": "./dist/forms/index.d.ts",
      "import": "./dist/forms/ui-kit.es.js",
      "require": "./dist/forms/ui-kit.cjs.js"
    },
    "./nav": {
      "types": "./dist/nav/index.d.ts",
      "import": "./dist/nav/ui-kit.es.js",
      "require": "./dist/nav/ui-kit.cjs.js"
    },
    "./layouts": {
      "types": "./dist/layouts/index.d.ts",
      "import": "./dist/layouts/ui-kit.es.js",
      "require": "./dist/layouts/ui-kit.cjs.js"
    },
    "./style.css": "./dist/ui-kit.css",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ui-kit.es.js",
      "require": "./dist/ui-kit.cjs.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.511.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@tailwindcss/vite": "^4.1.8",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-import-resolver-typescript": "^4.4.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "tailwindcss": "^4.1.8",
    "tw-animate-css": "^1.3.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4"
  }
}
